{
  "name": "sblp",
  "version": "1.0.4",
  "description": "SBLP provides an easy way to synchronize bumps over multiple bump bots.",
  "main": "index.js",
  "scripts": {
    "start": "ts-node-dev --inspect=9221 -- src/index.ts",
    "build": "npm run build:prepare && npm run build:copy && npm run build:tsc && npm run build:cleanup",
    "build:prepare": "rm -rf dist/build && mkdir -p dist/build",
    "build:copy": "cp -r src/* dist/build && mkdir -p config && cp -r config dist/build/ && cp package.json dist/build",
    "build:tsc": "tsc --project . --outDir dist/build",
    "build:cleanup": "rm -r dist/build/*.ts && rm -r dist/build/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lyne/open-bump.git"
  },
  "author": "Looat",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lyne/open-bump/issues"
  },
  "homepage": "https://blog.discord.one/open-bump/docs/topics/sblp/",
  "dependencies": {
    "@koa/router": "^9.3.1",
    "bcryptjs": "^2.4.3",
    "config": "^3.3.1",
    "dblapi.js": "^2.3.0",
    "discord-command-parser": "^1.3.3",
    "discord.js": "^12.1.1",
    "express": "^4.17.1",
    "express-rate-limit": "^5.0.0",
    "express-slow-down": "^1.3.1",
    "form-data": "^2.5.1",
    "koa": "^2.13.0",
    "koa-bodyparser": "^4.3.0",
    "koa-connect": "^2.1.0",
    "koa-passport": "^4.1.3",
    "koa-send": "^5.0.1",
    "koa-session": "^6.0.0",
    "koa-static": "^5.0.0",
    "mariadb": "^2.3.1",
    "moment": "^2.24.0",
    "mongoose": "^5.8.1",
    "mongoose-findorcreate": "^3.0.0",
    "ms": "^2.1.2",
    "mysql2": "^2.1.0",
    "node-fetch": "^2.6.1",
    "passport": "^0.4.1",
    "passport-discord": "^0.1.4",
    "patreon": "^0.4.1",
    "reflect-metadata": "^0.1.13",
    "semver": "^7.2.1",
    "semver-regex": "^3.1.1",
    "sequelize": "^5.21.6",
    "sequelize-typescript": "^1.1.0",
