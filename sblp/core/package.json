    "@types/koa-passport": "^4.0.2",
    "@types/koa-send": "^4.1.2",
    "@types/koa-session": "^5.10.2",
    "@types/koa-static": "^4.0.1",
    "@types/koa__router": "^8.0.2",
    "@types/ms": "^0.7.31",
    "@types/node": "^13.11.1",
    "@types/node-fetch": "^2.5.6",
    "@types/passport": "^1.0.4",
    "@types/passport-discord": "^0.1.3",
